package JavaProgramsUpdates;

 class SingleInheritance1 {
    public void show()
    {
    System.out.println("Hello");    
    }
}

class sandesh4 extends SingleInheritance1
{
public void display() 
{
System.out.println("bye");
}

public class Runner{

	public void main(String[] args) {
		sandesh obj = new sandesh();

		obj.show();
		obj.display();
		}  


	}

}
