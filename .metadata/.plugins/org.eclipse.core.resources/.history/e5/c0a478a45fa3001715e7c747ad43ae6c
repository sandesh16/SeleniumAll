package JavaProgramsUpdates;

public class singleInheritance {
	
	public class test
	{
		test()
		{
			
		}
		public void show() 
		{
		 System.out.println("Hello");	
		}
	}
	
	
	
	public class sandesh extends test
	{
		sandesh()
		{
			
		}
		
		public void display() 
		{
		 System.out.println("bye");	
		}
		
	}

	public static void main(String[] args) {
		
		sandesh obj = new sandesh();
		
		obj.show();
		obj.display();
	}

}
