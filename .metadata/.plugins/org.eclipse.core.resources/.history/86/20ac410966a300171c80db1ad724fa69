package JavaProgramsUpdates;

class singleInheritance {


	public void display() 
	{
	 System.out.println("bye");	
	}
	
}

  public class sandesh extends singleInheritance {
public void show() {
System.out.println("bye");
}


	public static void main(String[] args) {
		
		sandesh s=new sandesh();
		
		s.display();
		s.show();
		
	}

}
