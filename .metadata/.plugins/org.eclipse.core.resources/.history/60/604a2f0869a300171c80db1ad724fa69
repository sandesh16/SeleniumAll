package JavaProgramsUpdates;

 class SingleInheritance1 {
    public void show()
    {
    System.out.println("Hello");    
    }
}

class sandesh extends SingleInheritance1
{
public void display() 
{
System.out.println("bye");
}

class Main{

	public static void main(String[] args) {
		sandesh obj = new sandesh();

		obj.show();
		obj.display();
		}  


	}

}
