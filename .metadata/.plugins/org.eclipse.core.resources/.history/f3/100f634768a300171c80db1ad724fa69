package JavaProgramsUpdates;

class sandesh 
{

	public void display() 
	{
	 System.out.println("Hello");	
	}
	
}

public class singleInheritance extends sandesh {
	
public void show() 
{
System.out.println("bye");
}


	public static void main(String[] args) {
		
		singleInheritance s=new singleInheritance();
		
		s.display();
		s.show();
		
	}

}
