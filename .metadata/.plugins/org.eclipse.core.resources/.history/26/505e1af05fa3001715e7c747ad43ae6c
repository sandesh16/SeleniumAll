package JavaProgramsUpdates;


public class test
{
	test()
	{
		
	}
	public void show() 
	{
	 System.out.println("Hello");	
	}
}



public class sandesh extends test
{
	sandesh()
	{
		
	}
	
	public void display() 
	{
	 System.out.println("bye");	
	}
	
}

public class singleInheritance {
	

	public static void main(String[] args) {
		
		sandesh s=new sandesh();
		s.display();
		
	}

}
